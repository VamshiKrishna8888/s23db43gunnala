extends layout

block content
  h1= title
  
  - var image_names=["img1.jpeg","img2.jpeg","img3.png","img4.jpg","img5.png"]
  - for(let i=0;i<3;i++){
     - let randomNumber=Math.floor(Math.random()*5)
     img(src=`/images/${image_names[randomNumber]}` alt=`${randomNumber}` height="300px" width="300px") 
  - }
